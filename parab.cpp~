#include<iostream>
#include<cmath>


const double a=1;
const double b=1;
const double c=10;
    
double solucion1(const double a, const double b, const double c){
  double sol1 = (-b + sqrt(pow(b,2)-4*a*c))/(2*a);
      return sol1;
    }

  int main(void)
  {
  
    std::cout.precision(16);
    std::cout.setf(std::ios::scientific);
    double solucion1;  
    std::cout << solucion1 << "\n";
    return 0;
    
  }
